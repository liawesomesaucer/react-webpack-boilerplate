{
  "name": "react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "react-webpack-boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --config webpack.config.prod.js",
    "postinstall": "postinstall-build \"npm run build\"",
    "start": "NODE_ENV=production node server",
    "db": "NODE_ENV=development DEBUG=app:log nodemon --ignore app --ignore dist --exec babel-node server",
    "dev": "NODE_ENV=development webpack-dev-server --progress --inline --hot --history-api-fallback"
  },
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "postcss-smart-import": "^0.6.7",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.1",
    "webpack-dev-server": "2.2.1"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "keykey": "^2.1.1",
    "postinstall-build": "^2.1.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "webpack": "^2.2.1"
  }
}
